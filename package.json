{
  "name": "ansearch-landing",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "astro check && astro build",
    "dev": "astro dev",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "wrangler deploy",
    "lint": "echo 'ESLint'; yarn lint:eslint ; echo 'TSC'; yarn lint:tsc ; echo 'Prettier'; yarn lint:prettier ; echo 'Licenses'; yarn lint:licenses",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check --config .prettierrc.js",
    "lint:tsc": "tsc -p ./tsconfig.json",
    "lint:licenses": "yarn license-checker-rseidelsohn --onlyAllow 'MIT; MIT OR X11; BSD; ISC; Apache-2.0; Python-2.0; Unlicense; CC0-1.0; MPL-2.0; BlueOak-1.0.0; CC-BY-3.0; CC-BY-4.0; LGPL-3.0-or-later' --excludePackagesStartingWith '@astarlogic/;eslint-plugin-sonarjs;flatbuffers;posthog-js' --summary --unknown"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/ts-plugin": "^1.10.4",
    "@tailwindcss/vite": "^4.1.12",
    "astro": "^5.13.4",
    "astro-icon": "^1.1.5",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@ansearch/config": "^1.0.0",
    "@iconify-json/fa7-solid": "^1.2.1",
    "@iconify-json/flagpack": "^1.2.6",
    "@iconify-json/lucide": "^1.2.66",
    "@iconify-json/streamline": "^1.2.5",
    "@iconify-json/streamline-flex": "^1.2.3",
    "@iconify-json/streamline-plump": "^1.2.0",
    "@iconify-json/twemoji": "^1.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "license-checker-rseidelsohn": "^4.4.2",
    "prettier": "^3.6.2",
    "wrangler": "^4.35.0"
  },
  "packageManager": "yarn@4.9.4"
}
