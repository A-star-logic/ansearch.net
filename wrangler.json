{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ansearch-landing",
  "compatibility_date": "2025-09-09",
  "assets": {
    "directory": "./dist",
    "html_handling": "drop-trailing-slash"
  },
  "observability": {
    "enabled": true
  },
  "placement": { "mode": "smart" },
  "routes": [
    {
      "pattern": "ansearch.ai",
      "custom_domain": true
    },
    {
      "pattern": "ansearch.net",
      "custom_domain": true
    }
  ]
}
